{
  "mcpServers": {
    "local_bridge": {
      "url": "${MCP_BRIDGE_URL:-http://localhost:9000/mcp/}",
      "transport": "http",
      "enabled": false,
      "description": "Local MCP bridge for stdio servers (filesystem, mcp-this, etc.)"
    },
    "demo_tools": {
      "url": "http://fake-mcp-server:8001/mcp/",
      "transport": "http",
      "enabled": true
    },
    "render_demo_tools": {
      "url": "https://reasoning-agent-api-fake-mcp-server.onrender.com/mcp/",
      "transport": "http", 
      "enabled": false
    },
    "web_search": {
      "url": "https://mcp-web-search.example.com",
      "transport": "http",
      "auth_env_var": "WEB_SEARCH_API_KEY",
      "enabled": false
    },
    "database": {
      "url": "https://mcp-database.example.com",
      "transport": "http",
      "auth_env_var": "DATABASE_API_TOKEN",
      "enabled": false
    },
    "code_analyzer": {
      "url": "https://api.codeanalyzer.com/mcp",
      "transport": "http",
      "auth_env_var": "CODE_ANALYZER_KEY",
      "enabled": false
    }
  }
}
